package test;

import org.springframework.context.support.ClassPathXmlApplicationContext;

import assignment.spring.Customer;
import assignment.spring.CustomerDAOImpl;

class AssignmentTest {

	public static void main(String[] args) {
		ClassPathXmlApplicationContext ctx = new ClassPathXmlApplicationContext("spring-hib.xml");
		CustomerDAOImpl cs = ctx.getBean(CustomerDAOImpl.class);
		System.out.println(cs.getClass().getName());
		cs.update(new Customer("Raj", 20, "ambaji", "udaipur"));
		cs.getAll().forEach(c -> System.out.println(c));
	}
	
	
	
}
