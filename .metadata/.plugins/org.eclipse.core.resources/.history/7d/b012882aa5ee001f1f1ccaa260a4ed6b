package hibernate;

public class Tx {
	private long id;
	private double amount;
	
	public Tx() {}
	
	
	public Tx(double amount) {
		super();
		this.amount = amount;
	}


	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public double getAmount() {
		return amount;
	}
	public void setAmount(double amount) {
		this.amount = amount;
	}
	
}
